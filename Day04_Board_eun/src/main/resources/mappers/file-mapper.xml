<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="File">
	<insert id="insert">
		insert into files values(files_seq.nextval, #{writer}, #{oriname}, #{sysname}, #{parent_seq)
	</insert>


	<select id="getAllList" resultType="com.kedu.dto.FileDTO">
		select * from files order by seq
	</select>
</mapper>