package dto;

public class ContactsDTO {
	
	private int seq;
	private String name;
	private String phone;
	public int getSeq() {
		return seq;
	}
	public void setSeq(int seq) {
		this.seq = seq;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public ContactsDTO(int seq, String name, String phone) {
		this.seq = seq;
		this.name = name;
		this.phone = phone;
	}
	public ContactsDTO() {
	}
	
	
}
