<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Join">
	
	<select id="isIdExist" resultType="boolean">
		select id from members where id like #{id}
	</select>
	<select id="isIdExist" resultType="int">
		select count(*) from members where id = #{id} and pw = #{pw}
	</select>
	
	<insert id="insert">
		insert into members values(#{id}, #{pw}, #{name}, #{phone}, #{email}, #{zipcode}, #{address1}, #{address2}, sysdate)
	</insert>
	
	<delete id="deleteById">
		delete from members where id=#{id}
	</delete>

	<update id="">
	
	</update>
</mapper>